{
    "functions": {
      "api/**/*.ts": {
        "maxDuration": 60
      },
      "api/**/*.js": {
        "maxDuration": 60
      }
    },
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=3600, s-maxage=86400"
          }
        ]
      }
    ]
  }
  
  