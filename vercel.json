{
    "functions": {
      "app/api/**/*": {
        "maxDuration": 50
      }
    },
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=3600, s-maxage=86400"
          }
        ]
      }
    ]
  }
  
  